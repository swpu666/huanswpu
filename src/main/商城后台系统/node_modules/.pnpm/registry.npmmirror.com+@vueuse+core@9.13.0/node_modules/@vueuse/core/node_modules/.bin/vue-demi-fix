#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/代瑞/Desktop/商城后台系统/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/代瑞/Desktop/商城后台系统/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+vue-demi@0.14.5/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+vue-demi@0.14.5/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
fi
